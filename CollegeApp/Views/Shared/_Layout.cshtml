<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title - CollegeApp</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta content="width=device-width, initial-scale=1" name="viewport" />
    <meta content="" name="description" />
    <meta content="" name="author" />
    <!-- BEGIN GLOBAL MANDATORY STYLES -->
    <link href="http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700&subset=all" rel="stylesheet" type="text/css" />
    <link href='@Url.Content("~/Content/global/plugins/font-awesome/css/font-awesome.min.css")' rel="stylesheet" type="text/css" />
    <link href='@Url.Content("~/Content/global/plugins/simple-line-icons/simple-line-icons.min.css")' rel="stylesheet" type="text/css" />
    <link href='@Url.Content("~/Content/global/plugins/bootstrap/css/bootstrap.min.css")' rel="stylesheet" type="text/css" />
    <link href='@Url.Content("~/Content/global/plugins/uniform/css/uniform.default.css")' rel="stylesheet" type="text/css" />
    <link href='@Url.Content("~/Content/global/plugins/bootstrap-switch/css/bootstrap-switch.min.css")' rel="stylesheet" type="text/css" />
    <link href='@Url.Content("~/Content/global/plugins/bootstrap-toastr/toastr.min.css")' rel="stylesheet" type="text/css" />
    <!-- END GLOBAL MANDATORY STYLES -->
    <!-- BEGIN PAGE LEVEL PLUGIN STYLES -->
    <link href='@Url.Content("~/Content/global/plugins/gritter/css/jquery.gritter.css")' rel="stylesheet" type="text/css" />
    <link href='@Url.Content("~/Content/global/plugins/bootstrap-daterangepicker/daterangepicker-bs3.css")' rel="stylesheet" type="text/css" />
    <link href='@Url.Content("~/Content/global/plugins/fullcalendar/fullcalendar/fullcalendar.css")' rel="stylesheet" type="text/css" />
    <link href='@Url.Content("~/Content/global/plugins/jqvmap/jqvmap/jqvmap.css")' rel="stylesheet" type="text/css" />
    <link href='@Url.Content("~/Content/global/plugins/typeahead/typeahead.css")' rel="stylesheet" type="text/css" />

    <!-- END PAGE LEVEL PLUGIN STYLES -->
    <!-- BEGIN PAGE STYLES -->
    <link href='@Url.Content("~/Content/admin/pages/css/tasks.css")' rel="stylesheet" type="text/css" />
    <!-- END PAGE STYLES -->
    <!-- BEGIN THEME STYLES -->
    <link href='@Url.Content("~/Content/global/css/components.css")' rel="stylesheet" type="text/css" />
    <link href='@Url.Content("~/Content/global/css/plugins.css")' rel="stylesheet" type="text/css" />
    <link href='@Url.Content("~/Content/admin/layout/css/layout.css")' rel="stylesheet" type="text/css" />
    <link href='@Url.Content("~/Content/admin/layout/css/themes/light2.css")' rel="stylesheet" type="text/css" id="style_color" />
    <link href='@Url.Content("~/Content/admin/layout/css/custom.css")' rel="stylesheet" type="text/css" />
    <link href='@Url.Content("~/Content/Global/plugins/jquery-ui/jquery-ui-1.10.3.custom.min.css")' rel="stylesheet" type="text/css" />
    <link href='@Url.Content("~/Content/plugins/datepicker/datepicker3.css")' rel="stylesheet" type="text/css" />
    <script src='@Url.Content("~/Content/global/plugins/jquery-1.11.0.min.js")' type="text/javascript"></script>
    @RenderSection("customCSS", required: false)

    <!-- END THEME STYLES -->
    <link rel="shortcut icon" href="~/favicon.ico" type="image/x-icon" />

    <script>
        var rootPath = "@Url.Content("~")";
    </script>
</head>
<body class="page-header-fixed page-quick-sidebar-over-content">

    <!-- BEGIN HEADER -->
    <div class="page-header navbar navbar-fixed-top">
        <!-- BEGIN HEADER INNER -->
        <div class="page-header-inner">
            <!-- BEGIN LOGO -->
            <div class="page-logo">
                <a class="danger" href="/">
                    <h4>University of São Paulo</h4>
                </a>
                <div class="menu-toggler sidebar-toggler hide">
                    <!-- DOC: Remove the above "hide" to enable the sidebar toggler button on header -->
                </div>
            </div>
            <!-- END LOGO -->
            <!-- BEGIN RESPONSIVE MENU TOGGLER -->
            <a href="javascript:;" class="menu-toggler responsive-toggler" data-toggle="collapse" data-target=".navbar-collapse">
            </a>
            <!-- END RESPONSIVE MENU TOGGLER -->
            <!-- BEGIN TOP NAVIGATION MENU -->
            <div class="top-menu">
                @if (Request.IsAuthenticated)
                {
                    <ul class="nav navbar-nav pull-right">
                        @Html.Partial("_LoginPartial")
                    </ul>
                }
            </div>
            <!-- END TOP NAVIGATION MENU -->
        </div>
        <!-- END HEADER INNER -->
    </div>
    <!-- END HEADER -->
    <div class="clearfix">
    </div>

    <div class="page-container">
        @Html.Partial("_Sidebar")
        @RenderSection("featured", required: false)
        <section class="page-content-wrapper">
            <div class="page-content">
                @RenderBody()
            </div>
        </section>
        <footer>
            <div class="content-wrapper">
                <div class="version-footer">
                    <p>&copy; @DateTime.Now.Year - Midas Solutions</p>
                </div>
            </div>
        </footer>
        <div class="scroll-to-top" style="display: none;">
            <i class="icon-arrow-up"></i>
        </div>
    </div>

    <!-- END FOOTER -->
    <!-- BEGIN JAVASCRIPTS(Load javascripts at bottom, this will reduce page load time) -->
    <!-- BEGIN CORE PLUGINS -->
    <!--[if lt IE 9]>
    <script src='@Url.Content("~/Content/global/plugins/respond.min.js")'></script>
    <script src='@Url.Content("~/Content/global/plugins/excanvas.min.js")'></script>
    <![endif]-->
    <script src='@Url.Content("~/Content/global/plugins/jquery-migrate-1.2.1.min.js")' type="text/javascript"></script>
    <!-- IMPORTANT! Load jquery-ui-1.10.3.custom.min.js before bootstrap.min.js to fix bootstrap tooltip conflict with jquery ui tooltip -->
    <script src='@Url.Content("~/Content/global/plugins/jquery-ui/jquery-ui-1.10.3.custom.min.js")' type="text/javascript"></script>
    <script src='@Url.Content("~/Content/global/plugins/bootstrap/js/bootstrap.min.js")' type="text/javascript"></script>
    <script src='@Url.Content("~/Content/global/plugins/bootstrap-hover-dropdown/bootstrap-hover-dropdown.min.js")' type="text/javascript"></script>
    <script src='@Url.Content("~/Content/global/plugins/jquery-slimscroll/jquery.slimscroll.min.js")' type="text/javascript"></script>
    <script src='@Url.Content("~/Content/global/plugins/jquery.blockui.min.js")' type="text/javascript"></script>
    <script src='@Url.Content("~/Content/global/plugins/jquery.cokie.min.js")' type="text/javascript"></script>
    <script src='@Url.Content("~/Content/global/plugins/uniform/jquery.uniform.min.js")' type="text/javascript"></script>
    <script src='@Url.Content("~/Content/global/plugins/bootstrap-switch/js/bootstrap-switch.min.js")' type="text/javascript"></script>
    <!-- END CORE PLUGINS -->
    <!-- BEGIN PAGE LEVEL PLUGINS -->
    <script src='@Url.Content("~/Content/global/plugins/jqvmap/jqvmap/jquery.vmap.js")' type="text/javascript"></script>
    <script src='@Url.Content("~/Content/global/plugins/jqvmap/jqvmap/maps/jquery.vmap.russia.js")' type="text/javascript"></script>
    <script src='@Url.Content("~/Content/global/plugins/jqvmap/jqvmap/maps/jquery.vmap.world.js")' type="text/javascript"></script>
    <script src='@Url.Content("~/Content/global/plugins/jqvmap/jqvmap/maps/jquery.vmap.europe.js")' type="text/javascript"></script>
    <script src='@Url.Content("~/Content/global/plugins/jqvmap/jqvmap/maps/jquery.vmap.germany.js")' type="text/javascript"></script>
    <script src='@Url.Content("~/Content/global/plugins/jqvmap/jqvmap/maps/jquery.vmap.usa.js")' type="text/javascript"></script>
    <script src='@Url.Content("~/Content/global/plugins/jqvmap/jqvmap/data/jquery.vmap.sampledata.js")' type="text/javascript"></script>
    <script src='@Url.Content("~/Content/global/plugins/flot/jquery.flot.min.js")' type="text/javascript"></script>
    <script src='@Url.Content("~/Content/global/plugins/flot/jquery.flot.resize.min.js")' type="text/javascript"></script>
    <script src='@Url.Content("~/Content/global/plugins/flot/jquery.flot.categories.min.js")' type="text/javascript"></script>
    <script src='@Url.Content("~/Content/global/plugins/jquery.pulsate.min.js")' type="text/javascript"></script>
    <script src='@Url.Content("~/Content/global/plugins/bootstrap-daterangepicker/moment.min.js")' type="text/javascript"></script>
    <script src='@Url.Content("~/Content/global/plugins/bootstrap-daterangepicker/daterangepicker.js")' type="text/javascript"></script>
    <!-- IMPORTANT! fullcalendar depends on jquery-ui-1.10.3.custom.min.js for drag & drop support -->
    <script src='@Url.Content("~/Content/global/plugins/fullcalendar/fullcalendar/fullcalendar.min.js")' type="text/javascript"></script>
    <script src='@Url.Content("~/Content/global/plugins/jquery-easypiechart/jquery.easypiechart.min.js")' type="text/javascript"></script>
    <script src='@Url.Content("~/Content/global/plugins/jquery.sparkline.min.js")' type="text/javascript"></script>
    <script src='@Url.Content("~/Content/global/plugins/gritter/js/jquery.gritter.js")' type="text/javascript"></script>
    <script src='@Url.Content("~/Scripts/typeahead.bundle.min.js")' type="text/javascript"></script>
    <!-- END PAGE LEVEL PLUGINS -->
    <!-- BEGIN PAGE LEVEL SCRIPTS -->
    <script src='@Url.Content("~/Content/global/scripts/metronic.js")' type="text/javascript"></script>
    <script src='@Url.Content("~/Content/admin/layout/scripts/layout.js")' type="text/javascript"></script>
    <script src='@Url.Content("~/Content/admin/layout/scripts/quick-sidebar.js")' type="text/javascript"></script>
    <script src='@Url.Content("~/Content/admin/layout/scripts/demo.js")' type="text/javascript"></script>
    <script src='@Url.Content("~/Content/admin/pages/scripts/index.js")' type="text/javascript"></script>
    <script src='@Url.Content("~/Content/admin/pages/scripts/tasks.js")' type="text/javascript"></script>
    <script src='@Url.Content("~/Content/admin/pages/scripts/components-form-tools.js")' type="text/javascript"></script>
    <script src='@Url.Content("~/Content/plugins/datepicker/bootstrap-datepicker.js")' type="text/javascript"></script>
    <script src='@Url.Content("~/Content/global/plugins/bootstrap-toastr/toastr.min.js")' type="text/javascript"></script>
    <script src="~/Scripts/Portal/Generic.js"></script>
    @Scripts.Render("~/bundles/app")

    @RenderSection("scripts", required: false)

</body>
</html>
